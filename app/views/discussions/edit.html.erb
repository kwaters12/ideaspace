<%= form_for [@project, @discussion] do |f| %>
  <div class="form-group">
    <%= f.label :title, "Enter New discussion:" %>
    <br>
    <%= f.text_field :title, class: "form-control" %>
    
    <br>
    <%= f.text_area :body, class: "form-control"  %>
  </div>
  <%= f.submit class: "btn btn-default" %>
<% end %>